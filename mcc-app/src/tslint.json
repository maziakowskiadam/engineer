{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "indent": [
            true,
            "spaces",
            4
        ],
        "member-ordering": false,
        "no-string-literal": false
    }
}